{
  "name": "ngx-rut-demo",
  "description": "RUT for Angular",
  "version": "7.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "format:check": "prettier --check \"{projects,src/app}/**/*.ts\"",
    "format:write": "prettier --write \"{projects,src/app}/**/*.ts\"",
    "test:coverage": "ng test ngx-rut --code-coverage --watch=false",
    "report": "cat ./coverage/ng-alert/lcov.info | codecov"
  },
  "author": "Nicol√°s Leporati <nico.leporati@gmail.cl>",
